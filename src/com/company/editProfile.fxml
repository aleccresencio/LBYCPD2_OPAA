<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.EditProfileController">
   <children>
      <VBox layoutX="221.0" layoutY="44.0" prefHeight="501.0" prefWidth="558.0">
         <children>
            <HBox alignment="BOTTOM_CENTER" prefHeight="162.0" prefWidth="558.0">
               <children>
                  <Label fx:id="usernameLabel" styleClass="welcomeTitleLabel" text="Label" />
               </children>
            </HBox>
            <HBox alignment="TOP_CENTER" prefHeight="74.0" prefWidth="558.0">
               <children>
                  <Label fx:id="userRole" styleClass="titleLabel" text="Label" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="131.0" prefWidth="558.0">
               <children>
                  <Button fx:id="changePasswordButton" mnemonicParsing="false" onAction="#passwordButton" prefHeight="60.0" prefWidth="240.0" styleClass="loginButton" text="CHANGE PASSWORD" />
               </children>
            </HBox>
            <HBox alignment="TOP_CENTER" prefHeight="58.0" prefWidth="558.0">
               <children>
                  <Button fx:id="backButton" mnemonicParsing="false" onAction="#goBackButton" prefHeight="58.0" prefWidth="240.0" styleClass="loginButton" text="BACK TO MAIN MENU" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
    <stylesheets>
        <URL value="@opaa_stylesheet.css" />
    </stylesheets>
</AnchorPane>
